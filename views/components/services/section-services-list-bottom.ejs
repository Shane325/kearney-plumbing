<div class="section flv_sections_11 margin-top-50">
    <div class="section_wrapper clearfix">
        <div class="items_group clearfix">
            <% body.section_services_info.services_bottom.forEach(function(service) { %>
                <div class="column one-third column_column">
                    <div class="column_attr ">
                        <h3 class="themecolor">
                            <%= service.title %>
                        </h3>
                        <p>
                            <%= service.description %>
                        </p>
                    </div>
                </div>
                <% }) %>
        </div>
    </div>
</div>
