<!-- CTA 1 -->
<div class="call-to-action">
  <div class="container">
    <div class="column one column_column center-text">
      <span class="call-to-action-text hidden-on-mobile">
        <strong>Need plumbing services for your project?</strong>&nbsp;&nbsp;
      </span>
      <a class="button button_theme button_large button_js cta-button" href="/projects">
        <span class="button_label"><strong>SEE OUR WORK</strong></span>
      </a>
    </div>
  </div>
</div>
<!-- SERVICES -->
<div class="section flv_sections_16 margin-top-50" id="photobox">
  <div class="section_wrapper clearfix">
    <div class="items_group clearfix">
      <!-- One Third (1/3) Column -->
      <div class="column one-third column_photo_box">
        <div class="photo_box ">
          <h4>Residential</h4>
          <div class="image_frame">
            <div class="image_wrapper">
              <a href="/services">
                <div class="mask"></div><img class="scale-with-grid" src="<%= assets %>/home/home-residential.jpg" alt="Residential img" />
              </a>
            </div>
          </div>
          <div class="desc">
          </div>
        </div>
      </div>
      <!-- One Third (1/3) Column -->
      <div class="column one-third column_photo_box">
        <div class="photo_box ">
          <h4>Commercial</h4>
          <div class="image_frame">
            <div class="image_wrapper">
              <a href="/services">
                <div class="mask"></div><img class="scale-with-grid" src="<%= assets %>/home/home-commercial_2.jpg" alt="Commercial img" />
              </a>
            </div>
          </div>
          <div class="desc">
          </div>
        </div>
      </div>
      <!-- One Third (1/3) Column -->
      <div class="column one-third column_photo_box">
        <div class="photo_box ">
          <h4>Radiant Heat</h4>
          <div class="image_frame">
            <div class="image_wrapper">
              <a href="/services">
                <div class="mask"></div><img class="scale-with-grid" src="<%= assets %>/home/home-commercial.jpg" alt="Remodel img" />
              </a>
            </div>
          </div>
          <div class="desc">
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<!-- CTA 2 -->
<div class="section center flv_sections_22">
  <div class="section_wrapper clearfix">
    <div class="items_group clearfix">
      <!-- One full width row-->
      <div class="column one column_column">
        <div class="column_attr ">
          <a class="button button_left button_js button_large" href="/contact"><span class="button_icon"><i class="icon-clipboard"></i></span><span
                                                                                                                                                    class="button_label">SUBMIT BID REQUEST</span></a>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="section full-width flv_sections_16">
  <div class="section_wrapper clearfix">
    <div class="items_group clearfix">
      <!-- One full width row-->
      <div class="column one column_fancy_divider">
        <div class="fancy-divider">
          <svg preserveAspectRatio="none" viewBox="0 0 100 100" height="100" width="100%" version="1.1" xmlns="http://www.w3.org/2000/svg" class="flv_style_44">
            <path d="M0 0 L50 100 L100 0 Z" class="flv_style_45">
              </svg>
        </div>
      </div>
      <!-- Page devider -->
      <!-- One full width row-->
      <div class="column one column_divider">
        <hr class="no_line flv_style_21" />
      </div>
    </div>
  </div>
</div>
<!-- COUNTER -->
<div class="flv_sections_16 section margin-top-100" id="counter">
  <div class="section_wrapper clearfix">
    <div class="items_group clearfix">
      <!-- One full width row-->
      <div class="column one column_column">
        <div class="column_attr helper">
          <div class="helper_header">
            <h4 class="title">About us</h4>
          </div>
        </div>
      </div>
      <div class="column one-second column_counter">
        <div class="counter animate-math counter_horizontal">
          <div class="icon_wrapper">
            <i class="icon-cog"></i> </div>
          <div class="desc_wrapper">
            <div class="number-wrapper">
              <span class="number" data-to="38">0</span> </div>
            <p class="title">
              Projects completed </p>
          </div>
        </div>
      </div>
      <!-- One Third (1/3) Column -->
      <div class="column one-second column_counter">
        <div class="counter animate-math counter_horizontal">
          <div class="icon_wrapper">
            <i class="icon-tools"></i> </div>
          <div class="desc_wrapper">
            <div class="number-wrapper">
              <span class="number" data-to="16">0</span> </div>
            <p class="title">
              Years of experience</p>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<!-- PROJECT SLIDER -->
<div class="section" id="portfolioslider flv_sections_16">
  <div class="section_wrapper clearfix">
    <div class="items_group clearfix">
      <!-- One full width row-->
      <div class="column one column_column">
        <div class="column_attr helper">
          <div class="helper_header">
            <h4 class="title">Projects</h4>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="section full-width flv_sections_16">
  <div class="section_wrapper clearfix">
    <div class="items_group clearfix">
      <!-- One full width row-->
      <div class="column one column_portfolio_slider">
        <div class="portfolio_slider ">
          <a class="slider_nav slider_prev themebg" href="#"><i class="icon-left-open-big"></i></a><a class="slider_nav slider_next themebg" href="#"><i class="icon-right-open-big"></i></a>
          <ul class="portfolio_slider_ul">
            <% projects.forEach(function(project){ %>
              <li>
                <div class="image_frame scale-with-grid">
                  <div class="image_wrapper">
                    <a href="/project/<%= project.id %>">
                      <div class="mask"></div><img src="<%= project.portfolioImg.src %>" class="scale-with-grid wp-post-image" alt="<%= project.portfolioImg.alt %>" />
                    </a>
                    <div class="image_links single">
                      <a href="/project/<%= project.id %>" class="link"><i class="icon-link"></i></a>
                    </div>
                  </div>
                </div>
              </li>
              <% }) %>
          </ul>
        </div>
      </div>
    </div>
  </div>
</div>
<!-- TESTIMONIALS -->
