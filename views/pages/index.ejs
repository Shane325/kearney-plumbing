<!DOCTYPE html>
<!-- HTML5 -->

<html lang="en">

<head>
  <meta http-equiv="content-type" content="text/html; charset=utf-8" />

  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=0, viewport-fit=cover">

  <title>Kearney Plumbing |
    <%= pageTitle %>
  </title>

  <meta name="author" content="Shane Barry">
  <meta name="description" content="Welcome to Kearney Plumbing">
  <meta name="keywords" content="Construction,Commercial,Residential,Labor,Tools,Projects">

  <link rel='stylesheet' href='http://fonts.googleapis.com/css?family=Roboto:100,300,400,400italic,700'>
  <link rel='stylesheet' href='http://fonts.googleapis.com/css?family=Patua+One:100,300,400,400italic,700'>

  <% css.forEach(function(src){ %>
    <link rel="stylesheet" href="<%=src  %>" type="text/css">
    <% }) %>
</head>

<body class="page-parent template-slider color-blue layout-full-width header-modern subheader-transparent sticky-header sticky-white subheader-title-left">
  <div id="Wrapper">
    <div id="Header_wrapper">
      <header id="Header">
        <div id="Action_bar">
          <div class="container">
            <div class="column one">
              <ul class="contact_details">
                <li class="slogan">
                  Have any questions?
                </li>
                <li class="phone">
                  <i class="icon-phone"></i>
                  <a href="tel:<%= common.phone.src %>">
                    <%= common.phone.display %>
                  </a>
                </li>
                <li class="mail">
                  <i class="icon-mail-line"></i>
                  <a href="mailto:<%= common.email %>">
                    <%= common.email %>
                  </a>
                </li>
              </ul>
            </div>
          </div>
        </div>
        <div id="Top_bar">
          <div class="container">
            <div class="column one">
              <div class="top_bar_left clearfix">
                <div class="logo">
                  <a id="logo" href="/" title="Kearney Plumbing Inc."><img class="scale-with-grid" src="<%= common.assets %>/kearney-plumbing-logo.jpg"
                                                                           alt="Kearney Plumbing Logo" />
                  </a>
                </div>
                <div class="menu_wrapper">
                  <nav id="menu">
                    <ul id="menu-main-menu" class="menu">
                      <li class="<% if(state === 'home') { %>current-menu-item<% } %>">
                        <a href="/"><span>Home</span></a>
                      </li>
                      <li class="<% if(state === 'about') { %>current-menu-item<% } %>">
                        <a href="/about"><span>About us</span></a>
                      </li>
                      <li class="<% if(state === 'services') { %>current-menu-item<% } %>">
                        <a href="/services"><span>Services</span></a>
                      </li>
                      <li class="<% if(state === 'projects' || state === 'project') { %>current-menu-item<% } %>">
                        <a href="/projects"><span>Projects</span></a>
                        <ul class="sub-menu">
                          <li>
                            <a href="/projects"><span>Commercial</span></a>
                            <ul class="sub-menu">
                              <% var commercial = projects.filter(function(project) { %>
                                <% return project.categories === 'project-commercial' %>
                                  <% }) %>
                                    <% commercial.forEach(function(project) { %>
                                      <li>
                                        <a href="/project/<%= project.id %>"><span>
                                  <%= project.name %></span></a>
                                      </li>
                                      <% }) %>
                            </ul>
                          </li>
                          <li>
                            <a href="/projects"><span>Residential</span></a>
                            <ul class="sub-menu">
                              <% var residential = projects.filter(function(project) { %>
                                <% return project.categories === 'project-residential' %>
                                  <% }) %>
                                    <% residential.forEach(function(project) { %>
                                      <li>
                                        <a href="/project/<%= project.id %>"><span>
                                  <%= project.name %></span></a>
                                      </li>
                                      <% }) %>
                            </ul>
                          </li>
                          <li>
                            <a href="/projects"><span>Radiant Heat</span></a>
                            <ul class="sub-menu">
                              <% var radiantHeat = projects.filter(function(project) { %>
                                <% return project.categories === 'project-radiant-heat' %>
                                  <% }) %>
                                    <% radiantHeat.forEach(function(project) { %>
                                      <li>
                                        <a href="/project/<%= project.id %>"><span>
                                  <%= project.name %></span></a>
                                      </li>
                                      <% }) %>
                            </ul>
                          </li>
                          <li>
                            <a href="/projects"><span>Air to Water Heat</span></a>
                            <ul class="sub-menu">
                              <% var radiantHeat = projects.filter(function(project) { %>
                                <% return project.categories === 'project-air-to-water' %>
                                  <% }) %>
                                    <% radiantHeat.forEach(function(project) { %>
                                      <li>
                                        <a href="/project/<%= project.id %>"><span>
                                  <%= project.name %></span></a>
                                      </li>
                                      <% }) %>
                            </ul>
                          </li>
                        </ul>
                      </li>
                      <li class="<% if(state === 'contact') { %>current-menu-item<% } %>">
                        <a href="/contact"><span>Contact</span></a>
                      </li>
                      <li>
                        <a href="/air-to-water"><span>Air to Water</span></a>
                      </li>
                    </ul>
                  </nav><a class="responsive-menu-toggle" href="#"><i class="icon-menu"></i></a>
                </div>
              </div>
              <div class="top_bar_right">
                <div class="top_bar_right_wrapper">
                  <!-- Shopping cart icon-->
                  <a class="header_cart" href="tel:<%= common.phone.src %>"><i class="icon-phone"></i></a>
                  <a class="header_cart" href="mailto:<%= common.email %>"><i class="icon-mail"></i></a>
                  <a class="header_cart" href="<%= common.social.facebook %>" target="_blank"><i class="icon-facebook"></i></a>
                  <a class="header_cart" href="<%= common.social.instagram %>" target="_blank"><i class="icon-instagram"></i></a>
                </div>
              </div>
            </div>
          </div>
        </div>
        <% if(state === 'home'){ %>
          <% include ../partials/header/header-main-slider %>
            <% } %>
      </header>
    </div>
    <div id="Content">
      <div class="content_wrapper clearfix">
        <div class="sections_group">
          <div class="entry_content">
            <!-- CTA 1 -->
            <div class="call-to-action">
              <div class="container">
                <div class="column one column_column center-text">
                  <span class="call-to-action-text hidden-on-mobile">
                    <strong>Need plumbing services for your project?</strong>&nbsp;&nbsp;
                  </span>
                  <a class="button button_theme button_large button_js cta-button" href="/projects">
                    <span class="button_label"><strong>SEE OUR WORK</strong></span>
                  </a>
                </div>
              </div>
            </div>
            <!-- SERVICES -->
            <div class="section flv_sections_16 margin-top-50" id="photobox">
              <div class="section_wrapper clearfix">
                <div class="items_group clearfix">
                  <!-- One Third (1/3) Column -->
                  <div class="column one-third column_photo_box">
                    <div class="photo_box ">
                      <h4>Residential</h4>
                      <div class="image_frame">
                        <div class="image_wrapper">
                          <a href="/services">
                            <div class="mask"></div><img class="scale-with-grid" src="<%= assets %>/home-residential-2.jpg" alt="Residential img" />
                          </a>
                        </div>
                      </div>
                      <div class="desc">
                      </div>
                    </div>
                  </div>
                  <!-- One Third (1/3) Column -->
                  <div class="column one-third column_photo_box">
                    <div class="photo_box ">
                      <h4>Commercial</h4>
                      <div class="image_frame">
                        <div class="image_wrapper">
                          <a href="/services">
                            <div class="mask"></div><img class="scale-with-grid" src="<%= assets %>/home/home-commercial_2.jpg" alt="Commercial img" />
                          </a>
                        </div>
                      </div>
                      <div class="desc">
                      </div>
                    </div>
                  </div>
                  <!-- One Third (1/3) Column -->
                  <div class="column one-third column_photo_box">
                    <div class="photo_box ">
                      <h4>Radiant Heat</h4>
                      <div class="image_frame">
                        <div class="image_wrapper">
                          <a href="/services">
                            <div class="mask"></div><img class="scale-with-grid" src="<%= assets %>/home-radiant-heat-2.jpg" alt="Remodel img" />
                          </a>
                        </div>
                      </div>
                      <div class="desc">
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <!-- CTA 2 -->
            <div class="section center flv_sections_22">
              <div class="section_wrapper clearfix">
                <div class="items_group clearfix">
                  <!-- One full width row-->
                  <div class="column one column_column">
                    <div class="column_attr ">
                      <a class="button button_left button_js button_large" href="/contact"><span class="button_icon"><i class="icon-clipboard"></i></span><span
                                                                                                                                                    class="button_label">SUBMIT BID REQUEST</span></a>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <div class="section full-width flv_sections_16">
              <div class="section_wrapper clearfix">
                <div class="items_group clearfix">
                  <!-- One full width row-->
                  <div class="column one column_fancy_divider">
                    <div class="fancy-divider">
                      <svg preserveAspectRatio="none" viewBox="0 0 100 100" height="100" width="100%" version="1.1" xmlns="http://www.w3.org/2000/svg" class="flv_style_44">
                        <path d="M0 0 L50 100 L100 0 Z" class="flv_style_45">
                          </svg>
                    </div>
                  </div>
                  <!-- Page devider -->
                  <!-- One full width row-->
                  <div class="column one column_divider">
                    <hr class="no_line flv_style_21" />
                  </div>
                </div>
              </div>
            </div>
            <!-- COUNTER -->
            <div class="flv_sections_16 section margin-top-100" id="counter">
              <div class="section_wrapper clearfix">
                <div class="items_group clearfix">
                  <!-- One full width row-->
                  <div class="column one column_column">
                    <div class="column_attr helper">
                      <div class="helper_header">
                        <h4 class="title">About us</h4>
                      </div>
                    </div>
                  </div>
                  <div class="column one-second column_counter">
                    <div class="counter animate-math counter_horizontal">
                      <div class="icon_wrapper">
                        <i class="icon-cog"></i> </div>
                      <div class="desc_wrapper">
                        <div class="number-wrapper">
                          <span class="number" data-to="38">0</span> </div>
                        <p class="title">
                          Projects completed </p>
                      </div>
                    </div>
                  </div>
                  <!-- One Third (1/3) Column -->
                  <div class="column one-second column_counter">
                    <div class="counter animate-math counter_horizontal">
                      <div class="icon_wrapper">
                        <i class="icon-tools"></i> </div>
                      <div class="desc_wrapper">
                        <div class="number-wrapper">
                          <span class="number" data-to="16">0</span> </div>
                        <p class="title">
                          Years of experience</p>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <!-- PROJECT SLIDER -->
            <div class="section" id="portfolioslider flv_sections_16">
              <div class="section_wrapper clearfix">
                <div class="items_group clearfix">
                  <!-- One full width row-->
                  <div class="column one column_column">
                    <div class="column_attr helper">
                      <div class="helper_header">
                        <h4 class="title">Projects</h4>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <div class="section full-width flv_sections_16">
              <div class="section_wrapper clearfix">
                <div class="items_group clearfix">
                  <!-- One full width row-->
                  <div class="column one column_portfolio_slider">
                    <div class="portfolio_slider ">
                      <a class="slider_nav slider_prev themebg" href="#"><i class="icon-left-open-big"></i></a><a class="slider_nav slider_next themebg" href="#"><i class="icon-right-open-big"></i></a>
                      <ul class="portfolio_slider_ul">
                        <% projects.forEach(function(project){ %>
                          <li>
                            <div class="image_frame scale-with-grid">
                              <div class="image_wrapper">
                                <a href="/project/<%= project.id %>">
                                  <div class="mask"></div><img src="<%= project.portfolioImg.src %>" class="scale-with-grid wp-post-image" alt="<%= project.portfolioImg.alt %>" />
                                </a>
                                <div class="image_links single">
                                  <a href="/project/<%= project.id %>" class="link"><i class="icon-link"></i></a>
                                </div>
                              </div>
                            </div>
                          </li>
                          <% }) %>
                      </ul>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <!-- TESTIMONIALS -->
          </div>
        </div>
      </div>
    </div>
    <footer id="Footer" class="clearfix">
      <div class="footer_action">
        <div class="container">
          <div class="column one column_column">
            <span class="flv_color_white">
                <span class="flv_font_24 hidden-on-mobile"><strong>We're always available to answer any questions you may have</strong>&nbsp;&nbsp;
                </span>
            <a class="button button_large button_js footer-action-button" href="/contact">
                  <span class="button_label">CONTACT US</span>
                </a>
          </div>
        </div>
      </div>
      <div class="widgets_wrapper">
        <div class="container">
          <div class="one-third column">
            <aside id="text-7" class="widget widget_text footer-widget-about">
