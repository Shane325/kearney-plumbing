<div id="Action_bar">
    <div class="container">
        <div class="column one">
            <ul class="contact_details">
                <li class="slogan">
                    Have any questions?
                </li>
                <li class="phone">
                    <i class="icon-phone"></i>
                    <a href="tel:<%= common.phone.src %>">
                        <%= common.phone.display %>
                    </a>
                </li>
                <li class="mail">
                    <i class="icon-mail-line"></i>
                    <a href="mailto:<%= common.email %>">
                        <%= common.email %>
                    </a>
                </li>
            </ul>
        </div>
    </div>
</div>
